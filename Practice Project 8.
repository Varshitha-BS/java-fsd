
public class Conversion {
   public static void main(String args[]) {
      String strs[] = {"Sana", "Spoorthi", "Rohan", "Abhi" };
      StringBuilder sb = new StringBuilder();
      sb.append(strs[0]);
      sb.append(" "+strs[1]);
      sb.append(" "+strs[2]);
      sb.append(" "+strs[3]);
      System.out.println(sb.toString());
      
    
   }
    
	   }
